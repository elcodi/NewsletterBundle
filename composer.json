{
    "name": "elcodi/newsletter-bundle",
    "description": "Elcodi Newsletter Bundle",
    "keywords": ["elcodi", "newsletter", "ecommerce"],
    "homepage": "https://github.com/elcodi/NewsletterBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
